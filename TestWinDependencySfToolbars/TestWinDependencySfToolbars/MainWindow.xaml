<Window x:Class="TestWinDependencySfToolbars.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:syncfusion="http://schemas.syncfusion.com/wpf"
        xmlns:local="clr-namespace:TestWinDependencySfToolbars"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <syncfusion:ToolBarManager
            Name="toolBarManager"
            Grid.Row="1"
            FocusManager.IsFocusScope="False"
            Focusable="False"
            >
            <syncfusion:ToolBarManager.TopToolBarTray>
                <syncfusion:ToolBarTrayAdv VerticalAlignment="Top" FocusManager.IsFocusScope="True" Focusable="False" >
                    <syncfusion:ToolBarAdv ToolBarName="Toolbar1" >
                    </syncfusion:ToolBarAdv>
                    <syncfusion:ToolBarAdv ToolBarName="Toolbar2" >
                    </syncfusion:ToolBarAdv>
                </syncfusion:ToolBarTrayAdv>
            </syncfusion:ToolBarManager.TopToolBarTray>
        </syncfusion:ToolBarManager>
    </Grid>
</Window>
